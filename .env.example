# GitHub App Configuration
GITHUB_APP_ID=123456
GITHUB_PRIVATE_KEY="-----BEGIN RSA PRIVATE KEY-----\n...\n-----END RSA PRIVATE KEY-----"
GITHUB_WEBHOOK_SECRET=your_webhook_secret_here
GITHUB_INSTALLATION_ID=

# Service API Configuration
SERVICE_HMAC_SECRET=your_service_hmac_secret_here
BOT_API_TOKEN=your_bot_api_token_here
PORT=3000
NODE_ENV=development

# LlamaStack Configuration
LLAMA_URL=https://api.llamastack.com/v1
LLAMA_API_KEY=your_llama_api_key_here
LLAMA_MODEL=llama-3.1-70b
LLAMA_MAX_TOKENS=4096

# Queue & Database
REDIS_URL=redis://localhost:6379
DATABASE_URL=postgresql://llamaforge:llamaforge_dev@localhost:5432/llamaforge

# Feature Flags
DRY_RUN=false

# Observability (optional)
LOG_LEVEL=info
SENTRY_DSN=
